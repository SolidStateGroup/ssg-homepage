{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404" },
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/static/images/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "^/sitemap.xml",
      "dest": "/static/sitemap.xml",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    }
  ],
  "redirects": [
    { "source": "/our-work", "destination": "/work" }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ]
}

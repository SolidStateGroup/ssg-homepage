{
  "version": 2,
  "rewrites": [
    {
      "source": "^/service-worker.js$",
      "destination": "/_next/static/service-worker.js"
    },
    {
      "source": "^/favicon.ico$",
      "destination": "/static/images/favicon.ico"
    },
    {
      "source": "^/sitemap.xml",
      "destination": "/static/sitemap.xml"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404"
    }
  ],
  "redirects": [
    {
      "source": "/our-work",
      "destination": "/work"
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ]
}

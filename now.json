{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404" }
  ],
  "rewrites": [
    {
      "source": "^/service-worker.js$",
      "destination": "/_next/static/service-worker.js"
    },
    {
      "source": "^/favicon.ico$",
      "destination": "/static/images/favicon.ico"
    },
    {
      "source": "^/sitemap.xml",
      "destination": "/static/sitemap.xml"
    }
  ],
  "redirects": [
    { "source": "/our-work", "destination": "/work" }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ]
}
